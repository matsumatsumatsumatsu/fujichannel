<script async="" src="./thread_files/f(13).txt"></script>
<ins class="adsbygoogle" style="display: block; text-align: center; height: 185px;" data-ad-layout="in-article" data-ad-format="fluid" data-ad-client="ca-pub-6495453829303198" data-ad-slot="6094042708" data-adsbygoogle-status="done"><ins id="aswift_1_expand" style="display:inline-table;border:none;height:185px;margin:0;padding:0;position:relative;visibility:visible;width:737px;background-color:transparent;"><ins id="aswift_1_anchor" style="display:block;border:none;height:185px;margin:0;padding:0;position:relative;visibility:visible;width:737px;background-color:transparent;"><iframe width="737" height="185" frameborder="0" marginwidth="0" marginheight="0" vspace="0" hspace="0" allowtransparency="true" scrolling="no" allowfullscreen="true" onload="var i=this.id,s=window.google_iframe_oncopy,H=s&amp;&amp;s.handlers,h=H&amp;&amp;H[i],w=this.contentWindow,d;try{d=w.document}catch(e){}if(h&amp;&amp;d&amp;&amp;(!d.body||!d.body.firstChild)){if(h.call){setTimeout(h,0)}else if(h.match){try{h=s.upd(h,i)}catch(e){}w.location.replace(h)}}" id="aswift_1" name="aswift_1" style="left:0;position:absolute;top:0;border:0px;width:737px;height:185px;" src="./thread_files/saved_resource(10).html"></iframe></ins></ins></ins>
